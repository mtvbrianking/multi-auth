{
    "name": "bmatovu/multi-auth",
    "description": "Laravel Multi Authentication",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "multi",
        "auth"
    ],
    "support": {
        "issues": "https://github.com/mtvbrianking/multi-auth/issues",
        "source": "https://github.com/mtvbrianking/multi-auth"
    },
    "authors": [
        {
            "name": "Brian Matovu",
            "email": "mtvbrianking@gmail.com",
            "homepage": "https://bmatovu.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/notifications": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "code-lts/doctum": "^5.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bmatovu\\MultiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bmatovu\\MultiAuth\\Test\\": "tests/"
        }
    },
    "suggest": {
        "laravel/breeze": "Min. Laravel auth scaffolding with Blade and Tailwind."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-fix": "php-cs-fixer fix",
        "cs-lint": "php-cs-fixer fix --dry-run",
        "doc": "doctum.php update doctum.php -v",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.x-dev"
        },
        "laravel": {
            "providers": [
                "Bmatovu\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    }
}
