{
    "name": "bmatovu/multi-auth",
    "description": "Laravel Multi Authentication",
    "homepage": "https://github.com/mtvbrianking/multi-auth",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "multi",
        "authentication"
    ],
    "authors": [
        {
            "name": "Brian Matovu",
            "email": "mtvbrianking@gmail.com",
            "homepage": "http://bmatovu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/auth": "^6.0",
        "illuminate/console": "^6.0",
        "illuminate/notifications": "^6.0",
        "illuminate/routing": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bmatovu\\MultiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bmatovu\\MultiAuth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bmatovu\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    }
}
